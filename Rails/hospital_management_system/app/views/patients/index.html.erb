<h1 style="color:green;">List Of All Patients</h1>
<a href="/patients/new">Create New Patient</a>
<ol>
  <% @patients.each do |patient|%>
    <li> <%= link_to patient.name.titleize, "/patients/#{patient.id}" %> 
    &nbsp &nbsp<%= link_to "edit this patient", edit_patient_path(patient) %>
    &nbsp &nbsp <%= button_to "delete this patient", patient_path(patient), method: :delete %>
    </li><br>
  <%end%>  
</ol>

<%=link_to "previous", "/"%>